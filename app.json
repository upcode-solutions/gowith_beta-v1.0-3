{
  "expo": {
    "name": "gowith",
    "slug": "gowith",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./apps/assets/icons/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./apps/assets/icons/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./apps/assets/icons/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./apps/assets/icons/favicon.png"
    },
    "plugins": [
      "expo-build-properties"
    ]
  }
}
